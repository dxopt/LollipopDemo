<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="me.ycdev.android.demo.lollipopdemo">

    <uses-permission
        android:name="android.permission.PACKAGE_USAGE_STATS"
        tools:ignore="ProtectedPermissions" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="me.ycdev.android.demo.lollipopdemo.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.AppHome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".usage.apps.AppsUsageViewerActivity"
            android:label="@string/apps_usage_viewer" />

        <activity
            android:name=".usage.events.UsageEventsViewerActivity"
            android:label="@string/usage_events_viewer" />
    </application>

</manifest>
